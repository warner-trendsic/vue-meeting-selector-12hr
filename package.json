    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.0.23",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^3.2.11",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.9"
  }
