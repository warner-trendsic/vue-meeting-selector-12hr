    "@highlightjs/vue-plugin": "^2.1.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.18.11",
    "@vitejs/plugin-vue": "^3.1.2",
