    "./src/interfaces/Time.interface.ts": "./src/interfaces/Time.interface.ts"
  },
  "keywords": [
    "vue",
    "vuejs",
    "meeting-selector",
    "schedule-selector",
    "selector",
    "web-components"
  ],
  "private": false,
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build",
